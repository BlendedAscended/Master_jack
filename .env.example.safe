# Titan MCP Server - Environment Variables
# Copy this to .env and fill in your actual values

# ============== API KEYS ==============
APOLLO_API_KEY=your_apollo_api_key_here
AIRTABLE_API_KEY=your_airtable_pat_here
AIRTABLE_BASE_ID=your_airtable_base_id_here
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# ============== APIFY ==============
APIFY_API_TOKEN=your_apify_token_here

# ============== NOTION ==============
NOTION_API_KEY=your_notion_integration_token_here

# Database IDs (6-Database Architecture)
NOTION_INBOX_ID=your_inbox_db_id
NOTION_KNOWLEDGE_ID=your_knowledge_db_id
NOTION_PROJECTS_ID=your_projects_db_id
NOTION_TASKS_ID=your_tasks_db_id
NOTION_PEOPLE_ID=your_people_db_id
NOTION_CONTENT_ID=your_content_db_id

# ============== DATABASE ==============
# Agency Stack Postgres connection (shared with n8n/Windmill)
WINDMILL_DB_URL=postgresql://@db:5432/windmill

# ============== DISCORD ==============
DISCORD_BOT_TOKEN=your_discord_bot_token_here
DISCORD_CHANNEL_ID=your_channel_id_here
DISCORD_USER_ID=your_user_id_here
DISCORD_BRAIN_DUMP_CHANNEL_ID=your_brain_dump_channel_id_here

# ============== N8N INTEGRATION ==============
N8N_WEBHOOK_URL=https://agent.verbaflowllc.com
N8N_MCP_SERVER_URL=

# ============== GOOGLE DRIVE ==============
GDRIVE_FOLDER_ID=your_google_drive_folder_id_here
